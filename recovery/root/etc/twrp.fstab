/ramdisk            emmc      /dev/block/mmcblk0p42					                 flags=display="Boot Ramdisk";backup=1;
/cust               ext4      /dev/block/mmcblk0p53                  			     flags=display="Cust";backup=1;
/cust_image         emmc      /dev/block/mmcblk0p53                  				 flags=display="Cust Image";backup=1;flashimg=1;
/misc               emmc      /dev/block/mmcblk0p31                                  flags=display="Misc";backup=1;
